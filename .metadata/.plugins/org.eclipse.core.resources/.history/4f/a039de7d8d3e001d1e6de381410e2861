
public class ArraysDemo {
	
	String stu1 = "Engin";
    String stu2 = "Nursel";
    String stu3 = "Belinay";
    String stu4 = "Ahmet";
    


    System.out.println("--------------");

    String[] students = new String[3];
    students[0]="Engin";
    students[1]="Nursel";
    students[2]="Belinay";
    students[3]="Ahmet";

    for (int i=0; i<students.length ; i++){
        
    	System.out.println(students[i]);
    }

    System.out.println("--------------");

    for (String element: students){
        System.out.println(element);
    }



}
