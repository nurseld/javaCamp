
public class Loops {

	public static void main(String[] args) {
		
		//For Loop
		for (int i=2; i<10 ; i+=2 ) {
			
			System.out.println(i);
		}

		System.out.println("End of the for loop.");
		
		//While Loop
		
		int i = 1;
		while(i<10) {
			
			System.out.println("i");
			i++;
		}
		
		System.out.println("End of the while loop.");
	}

}
